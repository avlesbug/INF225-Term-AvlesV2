start:

funksjon add(c y) gjor:
    r er (c+y)
    , gir: r.

funksjon runderTilGrader(runde) gjor:
    r er runde*360
    , gir: r.

funksjon prosent(c y) gjor:
    skriv ut tekst "prosent"
    r er c/y*100
    , gir: r.


funksjon fibonacci(n) gjor:
    fst er 0
    snd er 1
    temp er 0
    fra 0 til n gjor:
        skriv ut fst
        temp er snd
        snd er add(fst snd)
        fst er temp
        .
    , gir: 0 .

funksjon rec(n) gjor:
    hvis n er mindre enn 1 gjor:
        svaret er 1
        ,ellers:
        svaret er rec(n-1)+rec(n-2).
    ,gir: svaret.




a er rec(3)

skriv ut a

slutt.